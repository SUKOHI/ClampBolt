{
    "name": "sukohi/clamp-bolt",
    "description": "A Laravel package that allows you to attach/detach files.",
    "authors": [
        {
            "name": "Sukohi",
            "email": "capilano.sukohi@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.7|^6.0|^7.0",
        "intervention/image": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Sukohi\\ClampBolt\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Sukohi\\ClampBolt\\ClampBoltServiceProvider"
            ]
        }
    }
}
